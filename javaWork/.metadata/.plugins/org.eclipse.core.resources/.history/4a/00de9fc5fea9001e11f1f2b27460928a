package test.com;

import java.util.Random;

public class Member {
	int num;
	String id;
	String pw;
	String name;
	String tel;

	public Member(int num, String id, String pw, String name, String tel) {
		super();
		this.num = num;
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.tel = tel;
	}

	public Member() {
		super();
	}

	String[] list() {
		
		Random random = new Random();
		int index = random.nextInt(5);
		return new String[] { num, id, pw, name, tel };
	}
}
